      "parameters": {
        "jsCode": "// --- ACAS DHRISTI FORCE-DETECTION ENGINE (V14) ---\nlet finalResults = [];\n\n// 1. DATA DRILLING\nlet rawData = items[0].json;\nlet cleanArray = [];\n\nif (Array.isArray(rawData)) {\n  cleanArray = rawData;\n} else if (rawData.body && Array.isArray(rawData.body)) {\n  cleanArray = rawData.body;\n} else if (rawData.data && Array.isArray(rawData.data)) {\n  cleanArray = rawData.data;\n} else {\n  cleanArray = [rawData];\n}\n\n// 2. PROCESSING EVERY PINCODE\nfor (let data of cleanArray) {\n  // --- NEW: Generate Unique Acknowledgment ID ---\n  let requestID = Math.random().toString(36).substring(2, 10) + Date.now().toString(36).slice(-4);\n\n  let stress = parseFloat(data.spatial_risk_index || 0);\n  let bio = parseFloat(data.total_bio_demand || 0);\n  let lead = parseFloat(data.student_demo_lead_7d || 0);\n  \n  let tier = \"LEVEL 1: STABLE\";\n  let priority = 1;\n  let analysis = \"Operational telemetry aligned with regional baseline.\";\n  let action = \"Standard operational maintenance.\";\n\n  if (stress > 50 && bio > 100) {\n    tier = \"LEVEL 5: EMERGENCY LOAD\";\n    priority = 5;\n    analysis = `CRITICAL: Infrastructure bottleneck at PIN ${data.pincode}.`;\n    action = \"Deploy Mobile Aadhaar Vans and pause non-mandatory updates.\";\n  } \n  else if (stress > 30 || bio > 150) {\n    tier = \"LEVEL 4: SATURATED OPS\";\n    priority = 4;\n    analysis = \"Peak biometric capacity reached at center.\";\n    action = \"Activate 2nd-Shift Verifiers.\";\n  } \n  else if (lead > 2.5) {\n    tier = \"LEVEL 3: MISSION MODE\";\n    priority = 3;\n    analysis = `Student enrolment surge (Lead: ${lead.toFixed(1)}) detected.`;\n    action = \"Dedicate Counter #1 for Student Mission.\";\n  } \n  else if (stress > 15) {\n    tier = \"LEVEL 2: ACTIVE MONITOR\";\n    priority = 2;\n    analysis = \"Low-level network pressure from neighboring nodes.\";\n    action = \"Monitor wait times; prepare local surge protocols.\";\n  }\n\n  // 3. RETURN INDIVIDUAL ITEMS\n  finalResults.push({\n    json: {\n      Request_ID: requestID, // --- ADDED FOR TRACKING ---\n      timestamp: new Date().toLocaleString(),\n      pincode: data.pincode ? data.pincode.toString() : \"N/A\",\n      district: data.district || \"N/A\",\n      operational_status: tier,\n      priority_level: priority,\n      risk_score: stress.toFixed(2),\n      audit_reasoning: analysis,\n      action_pathway: action,\n      identity: data.base_identity || \"General\",\n      ack_status: \"Pending\" // --- ADDED FOR GSheet INITIAL LOG ---\n    }\n  });\n}\n\nreturn finalResults;"
      },
